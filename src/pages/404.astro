---
import BaseLayout from '../layouts/BaseLayout.astro';
import NotFound from '../components/NotFound.astro';

let title = 'Uh Oh! Page Not Found';
let description =
  "We couldn't find the page you were looking for, go back to the home page.";
---

<BaseLayout
  {title}
  {description}
  canonicalURL={Astro.request.canonicalURL.href}
>
  <NotFound />
</BaseLayout>
